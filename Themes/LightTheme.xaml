<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Background Colors -->
    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#f3f3f3"/>
    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#FBFBFB"/>

    <!-- Primary Colors -->
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#6A7EFC"/>
    <SolidColorBrush x:Key="PrimaryBlueDarkColor" Color="#5A6DE7"/>
    <SolidColorBrush x:Key="PrimaryGrayColor" Color="#BFBFC4"/>
    
    <!-- Secundary Colors -->
    <SolidColorBrush x:Key="SecundaryBlueColor" Color="#CEE8FB"/>
    <SolidColorBrush x:Key="SecundaryGrayColor" Color="#EEEEEE"/>
    <SolidColorBrush x:Key="SecundaryLineGrayColor" Color="#B0B0B0"/>

    <!-- Typograhy Colors -->
    <SolidColorBrush x:Key="TextPrimaryColor" Color="#020F16"/>
    <SolidColorBrush x:Key="TextSecundaryColor" Color="#434344"/>

    <!-- Icons Colors -->
    <SolidColorBrush x:Key="ActionIconColor" Color="#434344"/>

    <!--Label Colors-->
    <SolidColorBrush x:Key="PrimaryLabelColor" Color="#494953"/>
    <SolidColorBrush x:Key="ActiveTextColor" Color="#6A7EFC"/>
    <SolidColorBrush x:Key="ActiveDarkTextColor" Color="#2438B7"/>
    <SolidColorBrush x:Key="DeleteTextColor" Color="#FF5656"/>
    <SolidColorBrush x:Key="H6TextColor" Color="#B0B0B0"/>

</ResourceDictionary>