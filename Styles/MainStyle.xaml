                          BorderThickness="{TemplateBinding BorderThickness}" Background="#fff"
                          Grid.ColumnSpan="2" IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}" 
